<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_medium_8dp"
        android:text="@string/title_welcome_back"
        android:layout_marginStart="@dimen/dimen_medium_8dp"
        android:textColor="@color/colorTextWhite"
        android:textStyle="bold"
        android:textSize="@dimen/text_size_medium_16sp"
        />
    <TextView
        android:layout_below="@id/title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dimen_medium_8dp"
        android:text="@string/title_admin"
        android:textStyle="bold"
        android:textColor="@color/colorTextWhite"
        android:textSize="@dimen/text_size_medium_16sp"/>

    <ImageView
        android:layout_width="200dp"
        android:layout_height="@dimen/frogo_rv_dimen_60dp"
        android:layout_marginStart="@dimen/dimen_big_16dp"
        android:src="@drawable/bg_new"
        android:layout_toEndOf="@id/title1"/>

</RelativeLayout>