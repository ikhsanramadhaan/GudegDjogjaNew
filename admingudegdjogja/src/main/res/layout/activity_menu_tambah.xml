<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_edit_text"
    android:backgroundTint="@android:color/transparent"
    tools:context=".view.activity.DetailMenuMakananActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Tambah Menu"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Foto"
        android:textColor="@color/colorPrimary"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/act_menu_tambah_makanan_foto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <ImageView
        android:id="@+id/act_menu_tambah_makanan_foto"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_dummy_photo" />

    <EditText
        android:id="@+id/act_menu_tambah_makanan_nama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bg_edit_text"
        android:ems="10"
        android:gravity="start|top"
        android:hint="@string/hint_nama_menu"
        android:inputType="textMultiLine"
        android:layout_marginStart="@dimen/dimen_big_ultra_32dp"
        android:layout_marginEnd="@dimen/dimen_big_ultra_32dp"
        android:paddingStart="@dimen/dimen_medium_8dp"
        android:paddingTop="@dimen/dimen_medium_8dp"
        android:paddingEnd="@dimen/dimen_medium_8dp"
        android:paddingBottom="@dimen/dimen_medium_8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.438"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="311dp"
        android:text="@string/title_nama_menu"
        android:textColor="@color/colorPrimary"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/act_menu_tambah_makanan_nama"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/act_menu_tambah_makanan_foto" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="320dp"
        android:text="@string/title_kategori_menu"
        android:textColor="@color/colorPrimary"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/act_menu_tambah_makanan_nama" />

    <Spinner
        android:id="@+id/act_menu_tambah_makanan_kategori"
        android:layout_width="339dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textview5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="316dp"
        android:text="@string/title_deskripsi_menu"
        android:textColor="@color/colorPrimary"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/act_menu_tambah_makanan_kategori" />

    <EditText
        android:id="@+id/act_menu_tambah_makanan_deskripsi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/bg_edit_text"
        android:ems="10"
        android:gravity="start|top"
        android:hint="@string/hint_nama_menu"
        android:inputType="textMultiLine"
        android:paddingStart="@dimen/dimen_medium_8dp"
        android:paddingTop="@dimen/dimen_medium_8dp"
        android:paddingEnd="@dimen/dimen_medium_8dp"
        android:paddingBottom="@dimen/dimen_medium_8dp"
        android:layout_marginStart="@dimen/dimen_big_ultra_32dp"
        android:layout_marginEnd="@dimen/dimen_big_ultra_32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.438"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview5" />

    <TextView
        android:id="@+id/textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:text="@string/title_harga_menu"
        android:textColor="#2B3D27"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/act_menu_tambah_makanan_harga"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.097"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/act_menu_tambah_makanan_deskripsi"
        tools:ignore="UnknownIdInLayout" />

    <EditText
        android:id="@+id/act_menu_tambah_makanan_harga"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_edit_text"
        android:ems="10"
        android:gravity="start|top"
        android:hint="@string/hint_nama_menu"
        android:inputType="textMultiLine"
        android:paddingStart="@dimen/dimen_medium_8dp"
        android:paddingTop="@dimen/dimen_medium_8dp"
        android:paddingEnd="@dimen/dimen_medium_8dp"
        android:paddingBottom="@dimen/dimen_medium_8dp"
        android:layout_marginStart="@dimen/dimen_big_ultra_32dp"
        android:layout_marginEnd="@dimen/dimen_big_ultra_32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview" />

    <ImageView
        android:id="@+id/menu_ubah_foto"
        android:layout_width="31dp"
        android:layout_height="26dp"
        android:padding="3dp"
        android:background="@drawable/bg_button"
        app:layout_constraintBottom_toTopOf="@+id/act_menu_tambah_makanan_nama"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.882"
        app:srcCompat="@drawable/ic_camera_alt_black_24dp" />

    <Button
        android:id="@+id/act_menu_tambah_btn_tambah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_tambah"
        android:background="@drawable/bg_button_update"
        android:textColor="@color/colorTextWhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/act_menu_tambah_makanan_harga"
        app:layout_constraintVertical_bias="0.111" />

</androidx.constraintlayout.widget.ConstraintLayout>